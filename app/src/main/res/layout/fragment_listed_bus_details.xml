<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/popup_style_set"
    android:padding="12dp"
    android:tag="ListedBusDetailsFragment">

    <TextView
        android:id="@+id/textView_result_buses_clicked_info_for_bus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/modernsans"
        android:text="@string/info_for_bus"
        android:textColor="@color/white"
        android:textSize="36sp"
        />

    <TextView
        android:id="@+id/textView_result_buses_clicked_arrives_in"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/modernsans"
        android:text="@string/arrives_in"
        android:textColor="@color/white"
        android:textSize="28sp"
        android:layout_below="@id/textView_result_buses_clicked_info_for_bus" />

    <TextView
        android:id="@+id/textView_result_buses_clicked_closest_station_arrive"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/modernsans"
        android:text="@string/get_to_this_station"
        android:textColor="@color/white"
        android:textSize="28sp"
        android:layout_below="@id/textView_result_buses_clicked_arrives_in" />

    <com.mapbox.mapboxsdk.maps.MapView
        android:id="@+id/mapView2"
        android:layout_width="240dp"
        android:layout_height="240dp"
        android:layout_margin="12dp"
        android:layout_below="@id/textView_result_buses_clicked_closest_station_arrive" />

</RelativeLayout>

